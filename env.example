# RoofVision GPU Measurement Service Configuration
# Copy this file to .env and fill in your values

# Google Solar API Key (for ground truth validation)
GOOGLE_SOLAR_API_KEY=your_google_solar_api_key_here

# S3 Storage (optional - for cloud photo storage)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
S3_BUCKET=roofvision-photos

# Redis (for job queue - defaults to localhost)
REDIS_URL=redis://localhost:6379

# Processing settings
MAX_CONCURRENT_JOBS=2
COLMAP_GPU_INDEX=0
